## Initialize brew auto completion ##
if [ -f $(brew --prefix)/etc/bash_completion ]; then
    . $(brew --prefix)/etc/bash_completion
fi

## Initialize sdkman
[[ -s "/Users/bene/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/bene/.sdkman/bin/sdkman-init.sh"

## Set gpg tty ##
export GPG_TTY=$(tty)
